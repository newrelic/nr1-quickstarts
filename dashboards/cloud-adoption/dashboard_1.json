        "key_names": [],
        "event_types": [
            "Transaction",
            "SyntheticCheck"
        ],
        "filter_first": false
    },
    "owner": {
        "email": "kdowns@newrelic.com"
    },
    "widgets": [
        {
            "title": " ",
            "nrql": null,
            "width": 4,
            "height": 6,
            "row": 1,
            "column": 1,
            "notes": null,
            "event_types": null,
            "facet": null,
            "agent_ids": [],
            "duration": null,
            "metrics": null,
            "raw_metric_name": null,
            "end_time": null,
            "cds_params_payload": null,
            "renderer": null,
            "renderer_props": null,
            "extension_type": null,
            "customizations": {
                "markdown": "![Image](https://s3.amazonaws.com/nr-cloud-adoption/CloudAdoption-Plan.png)\n"
            }
        },
        {
            "title": "Highest activity apps",
            "nrql": "SELECT count(*) FROM Transaction since 1 week ago FACET  appName",
            "process_as": "facet_pie_chart",
            "width": 4,
            "height": 3,
            "row": 1,
            "column": 5,
            "event_types": [
                "Transaction"
            ],
            "facet": "appName",
            "customizations": {
                "drilldown": {}
            },
            "notes": null
        },
        {
            "title": "Average Response Time by app",
            "nrql": "SELECT average(duration) FROM Transaction since 1 day ago FACET appName TIMESERIES AUTO",
            "process_as": "faceted_line_chart",
            "width": 4,
            "height": 3,
            "row": 1,
            "column": 9,
            "event_types": [
                "Transaction"
            ],
            "facet": "appName",
            "customizations": null,
            "notes": null
        },
        {
            "title": "Uptime SLA",
            "nrql": "SELECT percentage(count(*),where result = 'SUCCESS')   as 'Availability' from SyntheticCheck since 1 day ago COMPARE WITH 1 day ago",
            "process_as": "attribute_sheet",
            "width": 4,
            "height": 3,
            "row": 4,
            "column": 5,
            "event_types": [
                "SyntheticCheck"
            ],
            "facet": null,
            "customizations": null,
            "notes": null
        },
        {
            "title": "Application Response Codes",
            "nrql": "SELECT count(*) FROM Transaction since 1 day ago FACET  httpResponseCode",
            "process_as": "facet_pie_chart",
            "width": 4,
            "height": 3,
            "row": 4,
            "column": 9,
            "event_types": [
                "Transaction"
            ],
            "facet": "httpResponseCode",
            "customizations": {
                "drilldown": {}
            },
            "notes": null
        }
    ],
    "dashboard_account_id": 0
}
